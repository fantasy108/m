body{font-size:0.1574rem;background:#f3f3f3; }
a{text-decoration: none}
.tipbg{ position: fixed; width: 100%; height: 100%; background: #000; z-index: 90; top:0;background: rgba(0,0,0,0.5);display:none;}

.bgf3{ background:#f3f3f3 !important}

#wrapper {
    position:relative;
    z-index:1;
    bottom:0;
    left:0;
    width:100%;
    background:#fff;
    overflow:auto;
}

.order_list ul.booking li{float:left;background: #fff;margin:0 0 0.2249rem 0;border-top:solid 0.0112rem #eee;border-bottom:solid 0.0112rem #eee;padding:0 0.2249rem;}
.order_list ul.booking li h3{line-height: 0.4499rem;width:1.1249rem;float:left;}
.order_list ul.booking li .state{color:#ff642d;line-height: 0.4499rem;float:right;}
.order_list ul.booking li .oc_detail{float:left;padding:0.2249rem 0;border-top:solid 0.0112rem #eee;}
.order_list ul.booking li .oc_detail img{float:left;width:0.8999rem;border:solid 0.0112rem #eee;margin:0 0.1124rem 0 0;}
.order_list ul.booking li .oc_detail h4{float:left;color:#333;float:left;width:2.1149rem;height:0.4499rem;overflow: hidden;line-height: 0.2249rem}
.order_list ul.booking li .oc_detail h4 a{color:#333;}
.order_list ul.booking li .oc_detail .old_car_info{float:left;width:2.1149rem;}
.order_list ul.booking li .oc_detail p{float:left;color:#999;width:2.1149rem;line-height: 0.2249rem}
.order_list ul.booking li .operation{float:left;border-top:solid 0.0112rem #eee;padding:0.1124rem 0;width:3.1499rem;}
.order_list ul.booking li .operation a{float:right;border:solid 0.0112rem #0375e1;color:#0375e1;background: #fff;padding:0.0899rem 0.1124rem;border-radius:0.0562rem;margin:0 0 0 0.1124rem;display:block;}
.order_list ul.booking li .operation a.black{border:solid 0.0112rem #666;color:#666;}
.order_list ul.booking li .price{float:left;border-top:solid 0.0112rem #eee;line-height: 0.4499rem;width:3.1499rem}
.order_list ul.booking li .price span{float:right;color:#ff642d;line-height: 0.4499rem}
.order_list ul.booking li .price span.black{color:#333;font-size: 0.1574rem}

.error_layer{display:none;width:2.2499rem;height:auto;background: #fff;position:absolute;top:2.2499rem;left:0.6749rem;z-index: 100;border-radius:0.0562rem;}
.error_layer .low_high{line-height: 0.3374rem!important;margin:0.1124rem 0 0 0;}
.error_layer .error_txt{line-height: 0.6749rem;font-size: 0.1574rem;color:#333;border-bottom:solid 0.0112rem #eee;text-align: center}
.error_layer .correct{line-height: 0.4499rem;text-align: center;font-size: 0.1574rem;color:#2181f7;font-weight: bold;display:block;width:49%;float:left;border-right:solid 0.0112rem #eee;}
.error_layer .cancel{line-height: 0.4499rem;text-align: center;font-size: 0.1574rem;color:#2181f7;font-weight: bold;display:block;width:49%;float:left;}

.cancel_content{display:none;width:2.2499rem;height:auto;background: #fff;position:fixed;left:0.6749rem;z-index: 100;border-radius:0.0562rem;top: 2.2499rem;}
.cancel_content .low_high{line-height: 0.3374rem!important;margin:0.1124rem 0 0 0;}
.cancel_content .error_txt{line-height: 0.6749rem;font-size: 0.1574rem;color:#333;border-bottom:solid 0.0112rem #eee;text-align: center}
.cancel_content .correct{line-height: 0.4499rem;text-align: center;font-size: 0.1574rem;color:#2181f7;font-weight: bold;display:block;width:49%;float:left;border-right:solid 0.0112rem #eee;}
.cancel_content .cancel{line-height: 0.4499rem;text-align: center;font-size: 0.1574rem;color:#2181f7;font-weight: bold;display:block;width:49%;float:left;}



/*订单中心详情*/
.order_state{padding:0.2249rem;background: #fff;border-bottom:solid 0.0112rem #eee;margin:0 0 0.2249rem 0;}
.order_state h2{color:#ff642d;font:bold 0.2024rem/0.3374rem "微软雅黑";padding:0 0 0 0.5624rem;background: url('http://img.autostreetscdn.com/cmbwap/src/images/state.png') no-repeat;}
.order_state p{padding:0 0 0 0.5624rem;line-height: 0.3374rem}
.dealer_info h2{color:#999;line-height: 0.3374rem;text-indent: 0.2249rem;font-weight: normal}
.dealer_info ul{background: #fff;border-top:solid 0.0112rem #eee;border-bottom:solid 0.0112rem #eee;margin:0 0 0.2249rem 0;}
.dealer_info ul li{line-height: 0.4499rem;border-bottom:solid 0.0112rem #eee;padding:0 0.2249rem;position:relative;}
.dealer_info ul li:last-child{border:none;}
.dealer_info ul li label{width:0.8974rem;float:left;text-align: right;}
.dealer_info ul li p{overflow: hidden;}
.dealer_info ul li p img{margin:0.1124rem 0.0562rem 0 0;float:left;}
.dealer_info ul li p a span{color:#666;line-height: 0.4499rem;float:left;}
.dealer_info ul li .arrow{width:0.1124rem;height:0.1124rem;border-top: 0.025rem solid #ddd;border-right: 0.025rem solid #ddd;-webkit-transform:rotate(45deg);-moz-transform:rotate(45deg); transform:rotate(45deg);position:absolute;right:0.1124rem;top:0.1462rem;display:none;}
.order_time{padding:0.2249rem;margin:0 0 0.6749rem 0;}
.order_time p{color:#999;line-height: 0.2249rem}

.bottom{position:fixed;border-top:solid 0.0112rem #eee;background: #f7f7f7;height:0.4499rem;bottom:0;padding:0.1124rem;box-shadow: 0 0 0.0562rem #ddd;width:3.3749rem;}
.bottom a{float:right;border:solid 0.0112rem #0375e1;color:#0375e1;background: #fff;padding:0.0899rem 0.0562rem;border-radius:0.0562rem;margin:0 0 0 0.1124rem;display:block;}
.bottom a.black{border:solid 0.0112rem #666;color:#666;}

#divdown1{color:#ff642d;}

.refund_apply{padding:0rem 0 0 0; background: #fff;border-bottom:solid 1px #ddd; margin-bottom: 0.2rem}
.refund_apply textarea{background: #fff;color:#999;width:100%;resize:none;line-height: 0.2249rem;height:1.1249rem;font-size: 0.1799rem; border: 0px; padding: 0.1rem}
.refund_apply p{line-height: 0.3374rem;color:#999;text-align: right;padding:0 0.2249rem 0 0;}
.refund_apply p .limit{color:#f47410;}
.form_btn{width:3.1499rem;margin:0 0.2249rem;background: #0576E1;color:#fff;line-height: 0.4499rem;border:none;border-radius:0.0562rem;font-size: 0.1799rem;-webkit-appearance:none;}

.refuse_reason{padding:0.2249rem;}
.refuse_reason .ask p{text-align: center;color:#999;}
.refuse_reason .ask dl{border-radius:0.0562rem;background: #eee;width:2.9249rem;padding:0.1124rem;margin:0.1124rem 0 0.2249rem 0;}
.refuse_reason .ask dl dt{font:bold 0.1574rem/0.3374rem "宋体";border-bottom:solid 0.0112rem #ddd;color:#999;}
.refuse_reason .ask dl dd{font:normal 0.1349rem/0.2249rem "宋体";color:#999;margin:0.0562rem 0 0 0;}
.refuse_reason .answer p{text-align: center;color:#999;}
.refuse_reason .answer dl{border-radius:0.0562rem;background: #ff9326;width:2.9249rem;padding:0.1124rem;margin:0.1124rem 0 0.2249rem 0;}
.refuse_reason .answer dl dt{font:bold 0.1574rem/0.3374rem "宋体";border-bottom:solid 0.0112rem #fff;color:#fff;}
.refuse_reason .answer dl dd{font:normal 0.1349rem/0.2249rem "宋体";color:#fff;margin:0.0562rem 0 0 0;}

.refund_protocol{padding:0.2249rem;}
.refund_protocol h2{font:bold 0.2024rem/0.3374rem "微软雅黑";color:#333;text-align: center}
.refund_protocol p{line-height: 0.3374rem;margin:0.1124rem 0;color:#666;}
.refund_protocol p.last{margin:0 0 0.5624rem 0;}
.refund_protocol .call{width:3.1499rem;background: #fff;color:#0074e1;line-height:0.5624rem;text-align: center;display:block;font:normal 0.1799rem/0.5624rem "宋体";position:fixed;bottom:0.1124rem;border-radius:0.0562rem;border:solid 0.0112rem #eee;}
.refund_protocol .call img{ margin-right:0.1rem}

.home{width: 0.2812rem;height: 0.2812rem;display: inline-block;position: absolute;right: 0.15rem;top:0.0662rem;}
.home img{width:0.2812rem;height:0.2812rem;}
.paybox{ font-size:0.1574rem; padding-top: 0.1687rem}
.statusbody{ font-size:0.1574rem; padding-top: 0.1687rem}
.pay-sucess{background:url('http://img.autostreetscdn.com/cmbwap/src/images/success.png') no-repeat center top; background-size:0.675rem 0.675rem; padding:1rem 0.6rem 0rem 0.6rem ;text-align: center; margin-top:0.5rem; font-size: 0.18rem}
.pay-error{background:url('http://img.autostreetscdn.com/cmbwap/src/images/warning-2.png') no-repeat center top; background-size:0.675rem 0.675rem; padding:1rem 0.6rem 0rem 0.6rem ;text-align: center; margin-top:0.5rem; font-size: 0.18rem}
.status-btnbox a{ font-size: 0.1574rem; display: block;border: 1px solid #0576E1; background: #fff; margin:0 0.1125rem 0.1125rem 0.1125rem;height: 0.4612rem; line-height:0.4612rem;text-align: center; font-size:0.1874rem; border-radius: 0.05rem; color: #356fc4; text-decoration: none }
.status-btnbox a.tel{ color: #f47410;background:#fff url('http://img.autostreetscdn.com/cmbwap/src/images/phone-1.png') no-repeat 0.47rem center; background-size:0.225rem 0.225rem;}
.status-btnbox a.link1{ color: #0576E1;background:#fff; background-size:0.225rem 0.225rem;}
.pay-txt{ color: #999; text-align: center; margin:0.2rem 0}

