body {
    font-size:12px;
    -webkit-user-select:none;
    -webkit-text-size-adjust:none;
    font-family:helvetica;
}
#wrapper {
    position: relative;
    z-index: 1;
    bottom: 0;
    left: 0;
    width: 100%;
    background: #f6f6f6;
    overflow-x: hidden;
    overflow-y: auto;
}
#scroller_wrap{position:relative;}
#scroller{
    height:100%;
    /*float:left;*/
    padding:0;

    overflow:hidden;
    position:relative;
}

#scroller #thelist {
    list-style:none;
    display:block;
    float:left;
    width:100%;
    height:100%;
    padding:0;
    margin:0;
}

#scroller #thelist li {
    display:block; float:left;
    background:url(http://img.autostreetscdn.com/cmbwap/src/images/new_mall/loadingImg.gif) no-repeat left top;
    background-size:contain;
    height:100%;

}
#scroller li img{display:block;border:none;}

.no_pic {position:relative;}
.no_pic p{position:absolute;left:0.1124rem;bottom:0.2249rem;font-size:0.1574rem;color:#fff;font-family:"Microsoft YaHei";}
.no_pic img{display:block;border:none;}
.no_pic span{position:absolute;right:0.1124rem;bottom:0.2249rem;font-size:0.1574rem;color:#fff;font-family:"Microsoft YaHei";}

#nav {
    /*float:left;*/
    position:absolute;
    left:0;
    /*top:2.3624rem;*/

    bottom:0.1687rem;
    z-index: 99
}

#next {
    float:right;
    text-align:right;
}

#indicator, #indicator > li {
    display:block;
    list-style:none;
    padding:0; margin:0;
}

#indicator {
    width:100%;
    margin:0 auto;
    height:0.1124rem;
    text-align:center;
}

#indicator > li {
    width:8px;
    height:8px;
    border-radius:4px;
    background:#999;
    overflow:hidden;
    margin-right:4px;
    display:inline-block;
}

#indicator > li.active {
    background:#fff;
}

#indicator > li:last-child {
    margin:0;
}


