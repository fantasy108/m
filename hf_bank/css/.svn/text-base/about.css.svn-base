#about {
  font-size: 0.07875rem; }
#about #about_slide_wrap {
  position: relative; }
#about #about_slide ul li {
  float: left;
  display: block; }
#about #about_slide ul li img {
  width: 100%;
  height: auto;
  display: block;
  border: none; }
#about #indicator ul {
  position: absolute;
  bottom: 0.1125rem;
  text-align: center;
  width: 100%; }
#about #indicator ul li {
  width: 0.08437rem;
  height: 0.08437rem;
  background: #fff;
  display: inline-block;
  -webkit-border-radius: 50%;
  -moz-border-radius: 50%;
  border-radius: 50%;
  margin: 0.0225rem; }
#about #indicator ul > .active {
  background: #000; }
#about p {
  clear: both;
  text-indent: 2em;
  line-height: 0.225rem;
  margin: 0.16875rem;
  font-size: 0.135rem; }

#offline ul li {
  overflow: hidden;
  position: relative;
  padding: 0.1125rem;
  border-bottom: 1px solid #ddd; }
#offline ul li a {
  font-size: 0.16875rem;
  display: block;
  text-decoration: none;
  color: #666; }
#offline ul li img {
  float: left;
  width: 1.125rem;
  height: 0.5625rem;
  display: block;
  border: none;
  margin-right: 0.1125rem; }
#offline ul li .title {
