  margin-bottom: 0.05625rem; }
#offline ul li .addr {
  font-size: 0.135rem;
  line-height: 0.12937rem; }

.tel {
  display: block;
  border: 1px solid #DDDDDD;
  font-size: 0.18rem;
  color: #f27210;
  margin: 0.225rem 0.1125rem;
  height: 0.46125rem;
  -webkit-border-radius: 0.02812rem;
  -moz-border-radius: 0.02812rem;
  border-radius: 0.02812rem;
  text-decoration: none;
  line-height: 0.46125rem;
  text-indent: 0.93375rem;
  background: url(http://img.autostreetscdn.com/m/src/images/phone-1.png) no-repeat 0.54rem 0.11812rem;
  background-size: 0.225rem; }

.arrow {
  width: .1125rem;
  height: .1125rem;
  border-top: .025rem solid #ddd;
  border-right: .025rem solid #ddd;
  -webkit-transform: rotate(45deg);
  -moz-transform: rotate(45deg);
  transform: rotate(45deg);
  position: absolute;
  right: .1125rem;
  top: 50%;
  margin-top: -0.05625rem; }

