.halfbody{ background: #fff}
.halfbanner{}
.halfbanner img{ width: 100%}

.halflist{ font-size: 12px; overflow: hidden; padding:0.0562rem;padding-bottom:1rem; }
.halflist li{ float:left; width: 50%; padding:0.0562rem 0.0562rem 0.0562rem 0.0562rem; box-sizing:border-box; font-family:Arial;position: relative}
.halflist .li_inner{border:1px solid #e5e5e5;border-radius:5px;-webkit-border-radius:5px;-moz-border-radius:5px;height:2.2499rem;}
.halflist li a{ text-decoration: none}
.halflist li div{ background: #fff; padding:0.0562rem; }
.halflist li img{ width: 100%} 
.halflist li p{ background: #f5f5f5;font-size:0.1rem; height: 0.43rem; line-height: 0.15rem; padding:0.03rem 0.03rem; overflow: hidden; margin-bottom:0.05rem; color: #333}

.halflist li h3{ font-size:0.15rem; line-height: 0.18rem; height: 0.36rem; overflow: hidden; margin-bottom:0.05rem; color: #010101;}
.halflist li strong{ color: #e93e38; font-size: 0.15rem; margin-right: 0.1rem}
.halflist li del{ color: #999999; font-size:0.11rem}
