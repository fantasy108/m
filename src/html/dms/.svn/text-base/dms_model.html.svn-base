<!-- <script language="javascript" src="http://img.autostreetscdn.com/m/build/1.00/js/bm.js"></script> -->

<script>
$(function(){
    var exit_flag = false;
    $(".sm_hd").click(function(){
        $(".ui_mask").show().css("opacity","1")
        $(".sm_bd").show()
        if($('.select_module').length){
            var area_l = new IScroll('.area_l', {
                scrollbars: true,
                mouseWheel: true,
                interactiveScrollbars: true,
                shrinkScrollbars: 'scale',
                fadeScrollbars: true
            });
            var area_r = new IScroll('.area_r', {
                scrollbars: true,
                mouseWheel: true,
                interactiveScrollbars: true,
                shrinkScrollbars: 'scale',
                fadeScrollbars: true
            });
        }
    })
    $(".option").click(function(){
        $(this).children("a").show()
        exit_flag = true
        return false;
    })
    $(document).click(function(){
        if(exit_flag){
            $('.option>a').hide()
        }
    })
})
</script>
