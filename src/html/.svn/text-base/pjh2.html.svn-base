<script>
$(function(){
	$("#baoming").click(function(){
		if($(".name").val() == ""){
			alert("请输入姓名")
			return false;
		}
		if($(".phone").val() == ""){
			alert("请输入手机号")
			return false;
		}else if(!isMobile($(".phone").val())){
			alert("手机号格式错误")
			return false;
		}
		alert("ok")
	})
})

function　isMobile(s){
	var regex = /^0?(13[0-9]|15[0-9]|18[0-9]|14[0-9]|17[0-9])[0-9]{8}$/;
    if (!regex.exec(s)) return false
    return true
}
</script>

