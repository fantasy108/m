/*返回按钮*/
.back {
  display: block;
  height: 100%;
  width: 38px;
  position: absolute; }

.back::after {
  width: 12px;
  height: 12px;
  border-top: 1px solid #999;
  border-left: 1px solid #999;
  display: inline-block;
  position: absolute;
  left: 13px;
  -webkit-transform: rotate(-45deg);
  -moz-transform: rotate(-45deg);
  transform: rotate(-45deg);
  top: 14px;
  content: ' '; }

