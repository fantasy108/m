.nodata{ background:#f3f3f3 url('../images/nodata.png') no-repeat center 0.3rem;background-size: 1.4624rem 0.9562rem; padding-top: 1.5rem !important; border-bottom: 0px; text-align: center; color: #7d8592; font-size: 0.2rem !important; height: 3rem}

.loading{background: url('../images/loading.gif');
  background-size: 16px 16px;
  background-repeat: no-repeat;
  height: 32px;
  background-position: center center;
  position: fixed;
  top: 0;
  margin: auto;
  bottom: 0;
  width: 32px;
  left: 0;
  right: 0;}
