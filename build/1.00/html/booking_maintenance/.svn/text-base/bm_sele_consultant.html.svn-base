<script>
$(function(){
	var obj = request()
	sele_consultant(obj.orgSid)
	
	$('.consultant_list2>li').live('click',function(){
		var obj = $(this).children('i')
		if(obj.hasClass('unselect')){
			obj.removeClass('unselect').addClass('select').parent().siblings().children('i').removeClass('select').addClass('unselect')
		}
	})
	
	var selectCon = {}
	$('.find_him a').live('click',function(){
		$('.consultant_list2>li').each(function(){
			if($(this).children('i').hasClass('select')){
				selectCon.name = $(this).children('h3').text()
				selectCon.id = $(this).children('h3').attr('id')
				return false;
			}
		})
		$(this).attr('href','bm_time.html'+ location.search + '&consultSid='+selectCon.id +'&conName='+selectCon.name)
	})
})
</script>
